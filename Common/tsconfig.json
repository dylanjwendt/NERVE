{
  "extends": "@tsconfig/node14/tsconfig.json",
  "compilerOptions": {
    "outDir": "./build",
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es6",
    "allowJs": true,
    "esModuleInterop": true,
    "declaration": true,
    "sourceMap": true,
    "lib": ["DOM"],
    "experimentalDecorators": true,
    "resolveJsonModule": true
  }
}
